{
  "name": "babel-plugin-graphql",
  "version": "2.0.2",
  "description": "Babel plugin that compiles GraphQL tagged template strings",
  "author": "Florent Cailhol <florent.cailhol@gmail.com>",
  "license": "MIT",
  "repository": "ooflorent/babel-plugin-graphql",
  "keywords": [
    "babel-plugin",
    "graphql"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "graphql-parser": "^2.0.0",
    "lodash": "^3.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-eslint": "^3.1.5",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^0.21.1",
    "mocha": "^2.2.5"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib --copy-files",
    "watch": "npm run build -- --watch",
    "lint": "eslint src/",
    "test": "mocha",
    "prepublish": "npm run clean && npm run build"
  }
}
